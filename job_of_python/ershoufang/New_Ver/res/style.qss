QPushButton
{

    border-radius:4px;
    border:none;
    width:75px;
    height:25px;
    color:white;
    background:rgb(57,58,60);


}
QPushButton:enabled:hover
{
    background:rgb(230,39,39);
}
QPushButton:enabled:pressed
{
    background:rgb(255,0,0);
}
QPushButton:disabled
{

    background:rgb(100,100,100);
    color:rgb(200,200,200);
}